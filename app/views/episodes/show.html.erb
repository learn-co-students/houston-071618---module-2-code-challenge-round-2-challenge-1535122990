<h1>Episode</h1>

<h3>Guests: </h3>
<ul>
  <% @episode.appearances.each do |appearance| %>
    <li><%= link_to appearance.guest.name, guest_path(appearance.guest) %></li>
  <% end %>
</ul>


<h3>Number: </h3>
<ul>
    <li><%= @episode.number %></li>
</ul>

<h3>Date: </h3>
<ul>
    <li><%= @episode.date %></li>
</ul>
